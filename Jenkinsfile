pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('git clone') {
            steps {
                echo 'git cloning'
            }
        }
       stage('deploy') {
            steps {
                echo 'deploying'
            }
        }
       stage('testing') {
            steps {
                echo 'testing server'
            }
        }
        stage('docker') {
            steps {
                echo 'docker server'
            }
        }
     }   
}
